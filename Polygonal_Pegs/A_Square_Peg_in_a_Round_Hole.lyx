\begin_inset Formula 
\begin{eqnarray*}
w & = & v+\sqrt{1-v^{2}}\\
\left(w-v\right)^{2} & = & 1-v^{2}\\
2v^{2}-2wv+w^{2}-1 & = & 0\qquad\Rightarrow\\
v & = & \frac{w}{2}\pm\sqrt{\left(\frac{w}{2}\right)^{2}-\frac{w^{2}-1}{2}}\\
v & = & \frac{1}{2}\left(w\pm\sqrt{w^{2}-2w^{2}+2}\right)\\
v & = & \frac{1}{2}\left(w\pm\sqrt{2-w^{2}}\right)
\end{eqnarray*}

